<div id='signin'>
  <h1>sigin</h1>
  <form name="signinForm" ng-submit="submitForm(signinForm.$valid)" novalidate>
    <input type='text'ng-model='user.username' required ng-minlength="5"><label>I am an error message</label>
    <input type="password" ng-model='user.password' required ng-minlength="5">
    <button type="submit" ng-disabled="userForm.$invalid">signin</button>
    <p>I am an error message</p>
  </form>
  <a href="#/signup">Don't have an account? Signup ...</a>
</div>
